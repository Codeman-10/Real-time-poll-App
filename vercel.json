{
  "version": 2,
  "builds": [
    {
      "src": "client",
      "use": "@vercel/static-build"
    },
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/polls",
      "dest": "server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "client/index.html"
    }
  ]
}
